{
  "name": "zero-angular-workspace",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "ng build zero-angular",
    "build:lib": "ng build zero-angular",
    "watch": "ng build zero-angular --watch --configuration development",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "ng lint",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check",
    "changeset": "bunx changeset",
    "changeset:version": "bunx changeset version",
    "changeset:publish:dry": "bunx changeset publish --dry-run",
    "changeset:publish": "bun run build && npm publish dist/zero-angular --access public",
    "prepare": "bunx ng build zero-angular"
  },
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@typescript/native-preview": "^7.0.0-dev.20260115.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^21.1.0",
    "@angular/build": "^21.1.0",
    "@angular/cli": "^21.1.0",
    "@angular/compiler-cli": "^21.1.0",
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.28.0",
    "@rocicorp/zero": "^0.25.9",
    "@vitest/coverage-v8": "^4.0.17",
    "angular-eslint": "^21.1.0",
    "effect": "^3.12.0",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.1.0",
    "oxfmt": "^0.24.0",
    "oxlint": "^1.39.0",
    "oxlint-tsgolint": "^0.11.1",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  },
  "workspaces": [
    "projects/*",
    "builders"
  ],
  "packageManager": "bun@1.3.6"
}
